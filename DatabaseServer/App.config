        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="HOST" value="localhost"/>
      <add key="PORT" value="11000"/>
      <add key="BACKLOG" value="10"/>
      <add key="BUFFER" value="1024"/>
      <add key="STOP_MSG" value="Server stoped."/>
      <add key="TIMEOUT" value="10000"/>
      
      <add key="CREATE_DB" value="CRT DB"/>
      <add key="DROP_DB" value="DRP DB"/>
      <add key="USE_DB" value="USE DB"/>
      <add key="CREATE_TABLE" value="CRT TB"/>
