      <add key="INSERT_CONTENT" value="INS TB"/>
      <add key="GET_CONTENT" value="GET TB"/>

      <add key="OK_MSG" value="*** DONE ***"/>

      <add key="TABLE_CONTENT" value="TableContent"/>
      <add key="TABLE_META" value="TableStructure"/>
